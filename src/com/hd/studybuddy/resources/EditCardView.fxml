<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="230.0" prefWidth="396.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hd.studybuddy.view.editingDeck.EditCardViewController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="396.0" style="-fx-background-color: #b0dba8;">
         <items>
            <Label alignment="CENTER" prefHeight="40.0" prefWidth="380.0" text="Edit card">
               <font>
                  <Font name="Comic Sans MS" size="20.0" />
               </font></Label>
         </items>
      </ToolBar>
      <TextField fx:id="questionTextField" layoutX="95.0" layoutY="69.0" prefHeight="20.0" prefWidth="280.0" />
      <TextField fx:id="answerTextField" layoutX="95.0" layoutY="125.0" prefHeight="20.0" prefWidth="280.0" />
      <Label layoutX="28.0" layoutY="71.0" text="Question">
         <font>
            <Font name="Comic Sans MS" size="14.0" />
         </font></Label>
      <Label layoutX="33.0" layoutY="127.0" text="Answer">
         <font>
            <Font name="Comic Sans MS" size="14.0" />
         </font></Label>
      <Button fx:id="confirmBtn" layoutX="163.0" layoutY="177.0" mnemonicParsing="false" onAction="#confirmBtnClicked" text="Confirm" style="-fx-background-color: #b0dba8;">
         <font>
            <Font name="Comic Sans MS" size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
